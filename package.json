{
  "name": "video-playback",
  "version": "0.0.1",
  "description": "Checking video playback on different platforms",
  "scripts": {
    "tizen:build": "vite --config tizen/vite-config.js build --watch",
    "tizen:preview": "vite --config tizen/vite-config.js preview",
    "ts-node": "cross-env TS_NODE_PROJECT=\"tsconfig.ts-node.json\" TS_NODE_TRANSPILE_ONLY=true TS_NODE_SKIP_IGNORE=true node -r ts-node/register --max-old-space-size=4096",
    "tizen:run": "pnpm run ts-node tizen/run-app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Beraliv/video-playback.git"
  },
  "keywords": [
    "video",
    "playback"
  ],
  "author": "Alexey Berezin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Beraliv/video-playback/issues"
  },
  "homepage": "https://github.com/Beraliv/video-playback#readme",
  "devDependencies": {
    "@types/ip": "^1.1.0",
    "cross-env": "^7.0.3",
    "ip": "^1.1.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.3"
  }
}
